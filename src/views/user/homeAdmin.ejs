<main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">
    <!-- Navbar -->
    <!-- End Navbar -->
    <div class="container-fluid py-4">
        <div class="row mb-4">
            <div>
               <h6>Admin:<span> <h1 style="color:red"><%= data.nameAdmin.toUpperCase()%></h1></span></h6>
                <form action="">
                    <table class="table table-hover">
                        <thead>
                        <tr>
                            <th>Name</th>
                            <th>Admin</th>
                            <th>Action</th>
                        </tr>
                        </thead>
                        <tbody>
                        <% data.listUser.forEach( item=>{ %>
                            <form action="/auth/admin/grant" method="POST">
                                <input hidden name="nameUser" value="<%= item.username %>">
                                <tr>
                                    <td><%= item.username %></td>
                                    <td>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <label class="input-group-text" for="Admin">Options</label>
                                            </div>
                                            <select class="custom-select" id="Admin" name="Admin">
                                                <option value="choose" selected>Choose</option>
                                                <option value="false">User</option>
                                                <option value="true">Admin</option>
                                            </select>
                                        </div>


                                    </td>
                                    <td>
                                        <button type="button" class="btn btn-danger"><a href="/auth/admin/delete/<%= item.username %>" style="color:white;">Delete</a></button>
                                        <button type="submit" class="btn btn-success" name="<%= item.username %>" >submit</button>


                                    </td>
                                </tr>
                            </form>
                        <% }) %>
                        </tbody>
                    </table>
                </form>
            </div>

        </div>

    </div>
</main>
